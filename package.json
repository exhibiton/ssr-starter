{
  "name": "ssr-starter",
  "version": "0.0.1",
  "description": "SSR Starter",
  "main": "index.js",
  "engines": {
    "node": ">=9.11.1",
    "npm": "^5.6.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile:prod": "better-npm-run compile:prod",
    "compile": "better-npm-run compile",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploy": "better-npm-run deploy",
    "dev:build-server": "webpack --config build/webpack.server.config.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev": "npm-run-all --parallel dev:*",
    "fix:ts": "tslint --project tsconfig.json --config tslint.json --fix",
    "lint:all:fix": "npm run lint:all -- --fix",
    "lint:all": "eslint bin build config server src tests --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:test": "eslint test",
    "lint": "eslint src --ext js,jsx",
    "precommit": "lint-staged",
    "start": "better-npm-run start",
    "test:dev": "npm run test -- --watch",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "HOST": "localhost",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
